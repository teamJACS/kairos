{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "target": "es6",
    "emitDecoratorMetadata": true,  //for sequelize
    "experimentalDecorators": true, //for sequelize
    "types": ["node"]
  }
}
